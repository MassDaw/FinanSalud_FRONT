{
  "rewrites": [
    {
      "source": "/register",
      "destination": "/user_account_registration/user_register.html"
    },
    {
      "source": "/login",
      "destination": "/user_account_login/user_login.html"
    },
    { "source": "/dashboard", "destination": "/dashboard/dashboard.html" },
    { "source": "/profile", "destination": "/profile/profile.html" },
    { "source": "/overview", "destination": "/overview/monthly-overview.html" },
    { "source": "/market", "destination": "/market/market.html" },
    { "source": "/(.*)", "destination": "/$1" },
    { "source": "/navigation-bar","destination": "navigation-bar/navigation-bar.html" },
    { "source": "/footer", "destination": "footer/footer.html" }
  ]
}
