{
  "functions": {
    "api/auth-middleware.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/api/auth-middleware?page=dashboard"
    },
    {
      "source": "/profile",
      "destination": "/api/auth-middleware?page=profile"
    },
    {
      "source": "/monthly-overview",
      "destination": "/api/auth-middleware?page=monthly-overview"
    },
    {
      "source": "/market",
      "destination": "/api/auth-middleware?page=market"
    },
    {
      "source": "/register",
      "destination": "/user_account_registration/user_register.html"
    },
    {
      "source": "/login",
      "destination": "/user_account_login/user_login.html"
    },
    { "source": "/(.*)", "destination": "/$1" },
    {
      "source": "/navigation-bar",
      "destination": "navigation-bar/navigation-bar.html"
    },
    { "source": "/footer", "destination": "footer/footer.html" }
  ]
}
